                    <rect x="25" y="25" width="50" height="50" fill="url(#pattern0_3178_2162)"/>
                    <defs>
                        <pattern id="pattern0_3178_2162" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_3178_2162" transform="scale(0.00195312)"/>
