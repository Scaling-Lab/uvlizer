<style>
    .why-uvlizer-section{
        background: #EDF5F7;
        padding: 100px 0;
    }

    .why-uvlizer-section--container{
        max-width: 1440px;
        width: 100%;
        margin: auto;
        padding: 0 15px;
        text-align: center;
    }

    .why-uvlizer-section--container h1{
        color: #00436C;
        font-size: 36px;
        font-style: normal;
        font-weight: 600;
        line-height: 120%; /* 43.2px */
        text-transform: capitalize;
        margin-bottom: 10px !important;
    }

    .why-uvlizer-section--container p{
        color: #1F1F1F;
        text-align: center;
        font-size: 16px;
        font-style: normal;
        font-weight: 400;
        line-height: normal;
        max-width: 900px;
        margin: auto;
        display: inline-block;
    }

    .why-uvlizer-section__blocks{
        display: flex;
        column-gap: 25px;
        row-gap: 15px;
        margin-top: 40px;
    }

    .why-uvlizer-section__block{
        width: calc(25% - 25px);
        padding: 40px 0;
        border-radius: 10px;
        /*background: #FFF;*/
        /*box-shadow: 0px 0px 50px 0px rgba(0, 67, 108, 0.02);*/
    }

    .why-uvlizer-section__block svg {
        width: 100px;
        height: 100px;
    }

    .why-uvlizer-section__block h2{
        margin: 25px 0 10px 0 !important;
        color: #00436C;
        text-align: center;
        font-size: 20px;
        font-style: normal;
        font-weight: 600;
        line-height: 120%;
    }

    .why-uvlizer-section__block p {
        color: #1F1F1F;
        text-align: center;
        font-size: 16px;
        font-style: normal;
        font-weight: 400;
        line-height: normal;
    }

    .cta.fr {
        margin-top: 20px;
        margin-bottom: -20px;
    }

    @media screen and (max-width: 767px) {
        .why-uvlizer-section{
            padding: 50px 0;
        }

        .why-uvlizer-section--container h1{
            font-size: 24px;
        }

        .why-uvlizer-section--container p{
            font-size: 14px;
        }

        .why-uvlizer-section__blocks{
            flex-direction: column;
        }


        .why-uvlizer-section__block{
            width: 100%;
            padding: 0;
        }

        .why-uvlizer-section__block svg{
            width: 80px;
            height: 80px;
        }

        .why-uvlizer-section__block h2{
            font-size: 18px;
        }

        .why-uvlizer-section__block p{
            font-size: 14px;
        }
    }
</style>

