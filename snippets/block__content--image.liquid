
<img data-swift-lazy="1" loading="lazy" id="image--{{-block.id}}" src="{{block.settings.image | img_url: '10x'}}" class="lazyload"
data-src="{{block.settings.image | img_url: '1280x'}}" {{block.shopify_attributes}}>

{% comment %}
{% blockdef %}
{
    "type": "image",
    "name": "Image",
    "settings": [
        {
        "type": "checkbox",
        "id": "fullWidth",
        "default": true,
        "label": "Single in row"
        },
        {
            "id": "image",
            "label": "Image",
            "type": "image_picker"
        }
    ]
}
{% endblockdef %}
{% endcomment %}