{{ 'acc-uv-lp-02.css' | asset_url | stylesheet_tag }}

{% assign product = all_products[section.settings.product] %}
{% assign current_variant = product.selected_or_first_available_variant %}
{% assign featured_image = product.featured_image %}

<section class="uv-featured_product {% render 'common-class', section: section, class: 'row padding__vertical featured-product', vAlign: false %}">
  <div class="layout__content">
    <div class="title-wrapper">
      {% render 'block__general--heading' with section as section %}
    </div>
    <div class="featured-product_wrapper">
      <!-- Product Gallery Start -->

      <div class="featured-product_media-content">
        <div class="product-media-wrapper">
          <div class="product-slider_media">
            {% for image in product.images %}
              {% unless image.attached_to_variant? %}
                <span
                  data-variants="{% for var in image.variants %}{{var.id}}|{% endfor %}"
                  {% if forloop.first %}
                    class="active"
                  {% endif %}
                  data-media-type="image"
                  data-image-index="{{forloop.index0}}"
                  data-high-res-image="{{ image.src | img_url: 'original' }}"
                  data-href="{{ image.src | img_url: '650x' }}"
                  alt="{{image.alt | escape}}"
                  onclick="boosterProductPage.galleryChangeImage(event, this)"
                >
                  {% render 'image', size: '900x', src: image.src, alt: image.alt %}
                </span>
              {% endunless %}
            {% endfor %}
          </div>

          <div class="product-slider_thumbnails">
            {% for image in product.images %}
              {% unless image.attached_to_variant? %}
                <span
                  data-variants="{% for var in image.variants %}{{var.id}}|{% endfor %}"
                  {% if forloop.first %}
                    class="active"
                  {% endif %}
                  data-media-type="image"
                  data-image-index="{{forloop.index0}}"
                  data-high-res-image="{{ image.src | img_url: '1280x' }}"
                  data-href="{{ image.src | img_url: '650x' }}"
                  alt="{{image.alt | escape}}"
                  onclick="boosterProductPage.galleryChangeImage(event, this)"
                >
                  {% render 'image', size: '200x', src: image.src, alt: image.alt %}
                </span>
              {% endunless %}
            {% endfor %}
          </div>
        </div>
      </div>

      <!-- Product Gallery End -->

      <!-- Product Info Start -->

      <div class="featured-product_content">
        <div class="uv-featured_product-info">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'heading-with-review' %}
                <div class="featured-product_title" {{ block.shopify_attributes }}>
                  <span class="product-title"> {% render 'block__product-page--title' with product as product %}</span>
                </div>
              {% when 'icon-box' %}
                <div class="featured-product_benefits" id="block--{{ block.id }}" {{ block.shopify_attributes }}>
                  <div class="product_benefits-media">
                    {{ block.settings.image | image_url: width: 700 | image_tag: class: 'benefit_icon' }}
                  </div>
                  <div class="product_benefits-content">
                    {{ block.settings.text }}
                  </div>
                </div>
              {% when 'incart-watch' %}
                <div class="featured_product--incart" {{ block.shopify_attributes }}>
                  <ul class="product--incart_grid">
                    <li class="product--incart_item">
                      {% if block.settings.image1 != blank %}
                        <div class="product--incart_icon">
                          {{ block.settings.image1 | image_url: width: 700 | image_tag: class: 'cart_icon' }}
                        </div>
                      {% endif %}
                      {% render 'block__product-page--incart_new', block: block, product: product %}
                    </li>
                    <li class="product--incart_item">
                      {% if block.settings.image2 != blank %}
                        <div class="product--incart_icon">
                          {{ block.settings.image2 | image_url: width: 700 | image_tag: class: 'cart_icon' }}
                        </div>
                      {% endif %}
                      {% render 'block__product-page--watching_new', block: block, product: product %}
                    </li>
                  </ul>
                </div>
              {% when 'product-bundle' %}
                <div class="featured_product--bundle-wrapper bundle-wrapper ffff" {{ block.shopify_attributes }}>
                  <p class="bndl-title">{{ block.settings.text }}</p>
                  <div class="row-block">
                    <div
                      class="bundle-item blk pselection first-box b1 block--{{ block.id }} {% if produc.available %}available{% else %}not-availbe{% endif %}"
                      data-qty="1"
                      v_id="{{ product.variants.first.id }}"
                      data-variant="{{ variant.id }}"
                    >
                      <div class="blk b1 offer-box">
                        <p class="bprice">
                          {{
                            product.compare_at_price
                            | minus: product.price
                            | times: 100.0
                            | divided_by: product.compare_at_price
                            | round: 0
                          -}}
                          % OFF
                        </p>
                      </div>
                      {% comment %}<span class="d-none-selected-cart-style item-selected">SELECTED</span>{% endcomment %}
                      <div class="img-wrap">
                        <img src="https://cdn.shopify.com/s/files/1/0519/0238/2231/files/1n_png.png?v=1723452147">
                      </div>
                      <p class="btext_item">1 <span class="device-text">Device</span></p>
                      <p class="bprice new-price">
                        <span class="igPrice" data-product-id="{{product.id}}">
                          {{- product.price | times: 1 | money -}}
                        </span>
                      </p>
                      <p class="bprice old-price">
                        <span class="igComparePrice" data-product-id="{{product.id}}">
                          {{- product.compare_at_price | times: 1 | money -}}
                        </span>
                      </p>

                      <button
                        v_qty="1"
                        v_id="{{ product.variants.first.id }}"
                        class="d-none-selected-cart-style addcart btn-add-to-cart"
                      >
                        Add to cart
                      </button>
                    </div>
                    {% unless section.settings.productpack2 == blank %}
                      {% assign product2 = all_products[section.settings.productpack2] %}
                      {% if product2 %}
                        <div
                          class="bundle-item blk pselection second-box b1 {% if variant.available %}available{% else %}not-availbe{% endif %}"
                          data-qty="1"
                          v_id="{{ product2.variants.first.id }}"
                          data-variant="{{ variant.id }}"
                        >
                          <div class="blk b1 offer-box">
                            <p class="bprice">
                              {{
                                product2.compare_at_price
                                | minus: product2.price
                                | times: 100.0
                                | divided_by: product2.compare_at_price
                                | round: 0
                              -}}
                              % OFF
                            </p>
                          </div>
                          {% comment %}<span class="item-selected d-none-selected-cart-style">SELECTED</span>{% endcomment %}
                          <div class="img-wrap">
                            <img src="https://cdn.shopify.com/s/files/1/0519/0238/2231/files/1n.png_1.png?v=1723452517">
                          </div>
                          <p class="btext_item">2 <span class="device-text"> Devices</span></p>
                          <p class="bprice new-price">
                            <span class="igPrice" data-product-id="{{product2.id}}">
                              {{- product2.price | money -}}
                            </span>
                          </p>
                          <p class="bprice old-price">
                            <span class="igComparePrice" data-product-id="{{product2.id}}">
                              {{- product2.compare_at_price | money -}}
                            </span>
                          </p>

                          <button
                            v_qty="1"
                            v_id="{{ product.variants.first.id }}"
                            class="d-none-selected-cart-style addcart btn-add-to-cart"
                          >
                            Add to cart
                          </button>
                        </div>
                      {% endif %}
                    {% endunless %}

                    {% unless section.settings.productpack3 == blank %}
                      {% assign product3 = all_products[section.settings.productpack3] %}
                      {% if product3 %}
                        <div
                          class="bundle-item blk pselection third-box b1 {% if variant.available %}available{% else %}not-availbe{% endif %}"
                          data-qty="1"
                          v_id="{{ product3.variants.first.id }}"
                          data-variant="{{ variant.id }}"
                        >
                          <div class="blk b1 offer-box">
                            <p class="bprice">
                              {{
                                product3.compare_at_price
                                | minus: product3.price
                                | times: 100.0
                                | divided_by: product3.compare_at_price
                                | round: 0
                              -}}
                              % OFF
                            </p>
                          </div>
                          {% comment %}<span class="d-none-selected-cart-style item-selected">SELECTED</span>{% endcomment %}
                          <div class="img-wrap">
                            <img src="https://cdn.shopify.com/s/files/1/0519/0238/2231/files/1n.png_2.png?v=1723452601">
                          </div>
                          <p class="btext_item">3 <span class="device-text"> Devices</span></p>
                          <p class="bprice new-price">
                            <span class="igPrice" data-product-id="{{product3.id}}">
                              {{- product3.price | money -}}
                            </span>
                          </p>
                          <p class="bprice old-price">
                            <span class="igComparePrice" data-product-id="{{product3.id}}">
                              {{- product3.compare_at_price | money -}}
                            </span>
                          </p>

                          <button
                            v_qty="1"
                            v_id="{{ product.variants.first.id }}"
                            class="d-none-selected-cart-style addcart btn-add-to-cart"
                          >
                            Add to cart
                          </button>
                        </div>
                      {% endif %}
                    {% endunless %}
                  </div>
                </div>
              {% when 'inventory-status' %}
                <div class="featured_product--inventory" {{ block.shopify_attributes }}>
                  <div class="inventoryWrapper ">
                    {% if current_variant.inventory_quantity > 0
                      and current_variant.inventory_management == 'shopify'
                    %}
                      {% assign current_stock = current_variant.inventory_quantity %}

                      {% if current_stock > 748 %}
                        {% assign stock = 748 %}
                      {% endif %}

                      {% if current_stock < 251 %}
                        {% assign stock = 251 %}
                      {% endif %}

                      {% if current_stock < 1 %}
                        {% assign stock = 10 %}
                      {% endif %}

                      <p class="inventorytext">
                        Only
                        <!-- <span class="rqty red_text">{{ stock }}</span> / <span>10,000</span> -->
                        <span class="red_text">620</span> / <span>10,000</span>
                        Discounted Pieces Left
                      </p>
                      {% assign total_stock = 10000 %}
                      {% if stock < total_stock %}
                        {% assign progress = stock | times: 100 | divided_by: total_stock %}
                      {% else %}
                        {% assign progress = 100 %}
                      {% endif %}

                      <div class="progress-bar {% if stock == 0 %}progress--empty{% elsif stock < total_stock %}progress--track {% else %} progress--full{% endif %}">
                        <span class="progress" style="width: 85%"></span>
                        <span class="progress_icon" style="left: calc(85% - 10px);">
                          {% render 'progress_icon' %}
                        </span>
                      </div>
                    {% endif %}
                  </div>
                </div>
              {% when 'payment-terms' %}
                <div class="featured_product--payment_terms" {{ block.shopify_attributes }}>
                  {% form 'product', product %}
                    <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                    {{ form | payment_terms }}
                  {% endform %}
                </div>
              {% when 'atc-btn' %}
                <div class="featured_product--atc-btn" {{ block.shopify_attributes }}>
                  <div class="btn-wrap d-none-selected-cart-style-hide">
                    <button v_qty="1" v_id="{{ product.variants.first.id }}" class="addcart btn-add-to-cart">
                      Add to cart
                    </button>
                    <span class="go-cart__trigger js-go-cart-trigger">
                      <span class="donly">View Cart</span>
                      <span class="count donly"
                        ><span class="donly">(</span
                        ><span class="js-go-cart-counter"
                          >(
                          {{- cart.item_count -}}
                          )</span
                        ><span class="donly">)</span></span
                      >
                    </span>
                  </div>
                </div>
              {% when 'payment-icon' %}
                <div class="featured_product--payment-icon" {{ block.shopify_attributes }}>
                  <div class="payment-text">
                    <span class="ptext">{{ block.settings.text }}</span>
                  </div>
                  <div class="payment-icons">
