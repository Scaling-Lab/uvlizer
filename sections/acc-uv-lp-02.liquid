<script>
  // Load files when page is loaded
  window.addEventListener('load', function() {
    // Load jquery first because is a dependency
    loadScript('https://code.jquery.com/jquery-3.5.1.min.js', false, function() {
      // Load slick slider library and the code as callback
      loadScript('{{ 'slick.js' | asset_url }}', true, function() {
        $(document).ready(function() {


            $('.product-slider_media').slick({
              slidesToShow: 1,
              slidesToScroll: 1,
              arrows: false,
              infinite: false,
              asNavFor: '.product-slider_thumbnails'
            });
            $('.product-slider_thumbnails').slick({
              slidesToShow: 4.5,
              slidesToScroll: 1,
              asNavFor: '.product-slider_media',
              arrows: false,
              infinite: false,
              focusOnSelect: true,
              draggable: true,
              swipeToSlide: true

            });


        });
      });

      // load index.js and add the code for creating a new GoCart instance
      loadScript('{{ 'index.js' | asset_url }}', true, function() {
        var goCart = new GoCart();
      });
    });
  });

