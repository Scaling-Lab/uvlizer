  function updateProgressBar(stock, totalStock, inventoryItem) {
      var progressPercentage;

      if (stock >= totalStock) {
          progressPercentage = 100;
      } else {
          progressPercentage = (stock / totalStock) * 100;
      }

      var progressBar = inventoryItem.querySelector('.progress');
      var progressIcon = inventoryItem.querySelector('.progress_icon');

      if (progressBar) {
          progressBar.style.width = progressPercentage + '%';
      }
      if (progressIcon) {
